        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <div class="input-group">
                    <form id="form-search" method="get" action="" role="form" class="form-inline">
                        <div class="form-group has-feedback">
                            <div class="input-group">
                                <input style="color: #e7e7e7 !important;" type="text" class="form-control form-inline" id="explore" placeholder="Explore"
                                    id="search-text-input">
                                <span class="input-group-btn">
                                    <button id="letsgo" class="btn btn-default" type="button">Let's Go!
                                        <span>
                                            <i class="fa fa-paper-plane-o ml-1"></i>
                                        </span>
                                    </button>
                                </span>
                            </div>
                        </div>
                    </form>
                </div>
            <ul class="nav navbar-nav navbar-right" id="nav-links">
                <li>
                    <a id="contact-about" href="/contact">Contact</a>
